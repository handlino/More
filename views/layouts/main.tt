<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title class="lipsum"></title>
    <link rel="shortcut icon" href="/static/img/favicon.ico">
    <link rel="stylesheet" type="text/css" href="/css/grid.css" media="screen">
    <link rel="stylesheet" type="text/css" href="/css/styles.css" media="screen">
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"></script>

    <script type="text/javascript" src="/javascripts/moretext.js"></script>
    <script type="text/javascript" src="/javascripts/application.js"></script>
</head>
<body>
    <div class="wrap container_12 clearfix">
        <div class="grid_12">
            <h1 class="lipsum"></h1>
            <p class="lipsum"></p>
        </div>

        <div class="grid_12">
            <div class="grid_4 alpha">
                <blockquote>
                    <span class="quote-mark">“</span>
                    <span class="lipsum"></span>
                    <span class="quote-mark">”</span>
                    <br/><span class="author"> — <a href="http://handlino.com">Handlino</a></span>

                </blockquote>
            </div>

            <div class="grid_8 omega">
                <h2 class="lipsum"></h2>
                <div class="lipsum(5)"></div>
            </div>
        </div>

        <div class="grid_12">

            <div class="grid_6 alpha">
                <h2 class="lipsum"></h2>
                <div class="lipsum(5)"></div>
            </div>
            <div class="grid_6 omega">
                <h2 class="lipsum"></h2>
                <div class="lipsum(5)"></div>
            </div>
        </div>

        <div class="grid_12">
            <div class="grid_4 alpha">
                <h2 class="lipsum"></h2>
                <div class="lipsum(5)"></div>
            </div>
            <div class="grid_4 omega">
                <h2 class="lipsum"></h2>
                <div class="lipsum(5)"></div>

            </div>
            <div class="grid_4 omega">
                <h2 class="lipsum"></h2>
                <div class="lipsum(5)"></div>
            </div>
        </div>


        <div class="grid_12">
            <div id="footer">

                <span class="fancy">Brought to you by</span><br /><a href="#">Handlino.com</a>
            </div>
        </div>
    </div>

    <div id="control">
        <ul>
            <li><a href="javascript:void(0);" id="button:regenerate-text">Regenerate Text</a></li>

        </ul>
    </div>
</body>
</html>
